{
    "Id":"$index",
 	"Table": "events",
	"IDColumns": [
        "id"
    ],
	"Source": {
		"Credentials": "mysql-e2e",
		"Descriptor": "[username]:[password]@tcp(${dbIP.mysql}:3306)/[dbname]?parseTime=true",
		"DriverName": "mysql",
		"Parameters": {
			"dbname": "db1"
		}
	},
	"Dest": {
		"Credentials": "mysql-e2e",
		"Descriptor": "[username]:[password]@tcp(${dbIP.mysql}:3306)/[dbname]?parseTime=true",
		"DriverName": "mysql",
		"Parameters": {
			"dbname": "db2"
		}
	},
	"Partition": {
	    "Columns": [
	        "event_type"
	    ],
	    "Threads": 1,
	    "ProviderSQL":"SELECT event_type FROM db1.events GROUP BY 1 ORDER BY 1"
	},
    "Chunk":{
        "Size": 4096,
        "SyncMode": "individual",
        "Threads": 1
    },
	"Transfer": {
        "EndpointIP": "127.0.0.1:8080",
        "WriterThreads": 2,
        "TempDatabase":"transfer",
        "BatchSize": 2048
    }
}