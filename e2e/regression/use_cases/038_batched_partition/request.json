{
    "Id":"$index",
    "Table": "events",
	"IDColumns": [
        "id"
    ],
	"Source": {
		"Credentials": "mysql-e2e",
		"Descriptor": "[username]:[password]@tcp(${dbIP.mysql}:3306)/[dbname]?parseTime=true",
		"DriverName": "mysql",
		"Parameters": {
			"dbname": "db1"
		}
	},
	"Dest": {
		"Credentials": "mysql-e2e",
		"Descriptor": "[username]:[password]@tcp(${dbIP.mysql}:3306)/[dbname]?parseTime=true",
		"DriverName": "mysql",
		"Parameters": {
			"dbname": "db2"
		}
	},
	"Diff": {
	    "BatchSize":2
	},
	"Partition": {
	    "BatchSize":2,
        "Columns": [
            "event_type"
        ],
        "SyncMode":"batch",
        "Threads": 2,
        "ProviderSQL":"SELECT event_type FROM db1.events GROUP BY 1 ORDER BY 1"
    },
    "Transfer": {
        "TempDatabase":"transfer",
        "EndpointIP": "127.0.0.1:8080",
        "WriterThreads": 2,
        "BatchSize": 2048
    }
}