{
    "Id":"$index",
    "Debug":true,
 	"Table": "events",
	"IDColumns": [
        "id"
    ],
    "Diff": {
        "NewIDOnly": true,
        "Columns": [
            {
                "Name":"timestamp",
                "Func":"MAX"
            }
        ]
    },

	"Source": {
		"Credentials": "mysql-e2e",
		"Descriptor": "[username]:[password]@tcp(${dbIP.mysql}:3306)/[dbname]?parseTime=true",
		"DriverName": "mysql",
		"Parameters": {
			"dbname": "db1"
		}
	},
	"Dest": {
		"Credentials": "mysql-e2e",
		"Descriptor": "[username]:[password]@tcp(${dbIP.mysql}:3306)/[dbname]?parseTime=true",
		"DriverName": "mysql",
		"Parameters": {
			"dbname": "db2"
		}
	},

    "Chunk":{
        "Size": 4096,
        "SyncMode": "individual"
    },
    "Transfer": {
        "EndpointIP": "127.0.0.1:8080",
        "WriterThreads": 2,
        "TempDatabase":"transfer",
        "BatchSize": 4096
    }
}