{
    "Id":"$index",
 	"Table": "events",
	"IDColumns": [
        "id"
    ],
    "Debug":true,

    "Diff": {
        "Depth":3,
        "Columns": [
            {
                "Name":"timestamp",
                "Func":"MAX"
            }
        ]
    },

	"Source": {
		"Credentials": "mysql-e2e",
		"Descriptor": "[username]:[password]@tcp(${dbIP.mysql}:3306)/[dbname]?parseTime=true",
		"DriverName": "mysql",
		"Parameters": {
			"dbname": "db1"
		}
	},
	"Dest": {
		"Credentials": "mysql-e2e",
		"Descriptor": "[username]:[password]@tcp(${dbIP.mysql}:3306)/[dbname]?parseTime=true",
		"DriverName": "mysql",
		"Parameters": {
			"dbname": "db2"
		}
	},

    "Chunk":{
        "Size": 6,
        "SyncMode": "individual",
        "Threads": 2
    },
    "Transfer": {
        "EndpointIP": "127.0.0.1:8080",
        "WriterThreads": 2,
        "TempDatabase":"transfer",
        "BatchSize": 3
    }
}