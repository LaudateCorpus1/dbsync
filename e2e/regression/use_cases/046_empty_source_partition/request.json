{
   "Id":"$index",
 	"Table": "events",
	"IDColumns": [
        "id"
    ],
    "Source": {
        "Credentials": "mysql-e2e",
        "Descriptor": "[username]:[password]@tcp(${dbIP.mysql}:3306)/[dbname]?parseTime=true",
        "DriverName": "mysql",
        "Parameters": {
            "dbname": "db1"
        },
        "PartitionSQL": "SELECT event_type FROM db1.events WHERE event_type = 4 GROUP BY 1 ORDER BY 1"
    },
	"Dest": {
		"Credentials": "mysql-e2e",
		"Descriptor": "[username]:[password]@tcp(${dbIP.mysql}:3306)/[dbname]?parseTime=true",
		"DriverName": "mysql",
		"Parameters": {
			"dbname": "db2"
		},
		"PartitionSQL": "SELECT event_type FROM db2.events WHERE event_type = 4 GROUP BY 1 ORDER BY 1"
	},
	"Partition": {
        "Columns": [
            "event_type"
        ],
        "Threads": 5
    },
    "Transfer": {
            "EndpointIP": "127.0.0.1:8080",
            "WriterThreads": 2,
            "BatchSize": 2048
    }
}