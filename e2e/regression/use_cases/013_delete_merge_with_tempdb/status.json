{
  "Method": "get",
  "URL": "http://localhost:8081/v1/api/job/$index",
  "Expect": {
    "Jobs": [
      {
        "Status": "done",
        "Progress": {
          "DestCount": 6,
          "SourceCount": 10,
          "Transferred": 10
        }
      }
    ]
  },
  "Repeat": 10,
  "SleepTimeMs": 2000,
  "Exit": "${Jobs[0].Status}:!/running/"
}
