{
    "Id": "$index",
    "Table": "events",
   	"Debug": true,

    "IDColumns": [
      "id"
    ],

    "Criteria": {
      "event_type": " <= 8 "
    },
	"Source": {
		"Credentials": "mysql-e2e",
		"Descriptor": "[username]:[password]@tcp(${dbIP.mysql}:3306)/[dbname]?parseTime=true",
		"DriverName": "mysql",
		"Parameters": {
			"dbname": "db1"
		}
	},
	"Dest": {
		"Credentials": "mysql-e2e",
		"Descriptor": "[username]:[password]@tcp(${dbIP.mysql}:3306)/[dbname]?parseTime=true",
		"DriverName": "mysql",
		"Parameters": {
			"dbname": "db2"
		}
	},
    "Diff": {
        "CountOnly": true
    },
    "Chunk":{
        "Size": 5
    },
	"Transfer": {
            "EndpointIP": "127.0.0.1:8080",
            "WriterThreads": 1,
            "TempDatabase":"transfer",
            "BatchSize": 2048
    }
}